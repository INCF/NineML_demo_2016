<?xml version='1.0' encoding='UTF-8'?>
<nineml:nineml xmlns:nineml="http://nineml.incf.org/9ML/0.3" xmlns="http://nineml.incf.org/9ML/0.3" name="Three-neuron network with alpha synapses">
  <nineml:component name="syn">
    <nineml:definition language="NineML">
      <nineml:link>AlphaPSR.xml</nineml:link>
    </nineml:definition>
    <nineml:property name="tau_syn">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>0.5</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:initial name="A">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>0.0</nineml:scalar>
          <nineml:unit>nA</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
    <nineml:initial name="B">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>0.0</nineml:scalar>
          <nineml:unit>nA</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
  </nineml:component>
  <nineml:component name="stim">
    <nineml:definition language="NineML">
      <nineml:link>Poisson.xml</nineml:link>
    </nineml:definition>
    <nineml:property name="rate">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>1000.0</nineml:scalar>
          <nineml:unit>Hz</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:initial name="t_next">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>0.0</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
  </nineml:component>
  <nineml:component name="ExternalPlasticity">
    <nineml:definition language="NineML">
      <nineml:link>StaticConnection.xml</nineml:link>
    </nineml:definition>
    <nineml:property name="delay">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>1.5</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:initial name="t_next">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>1000000000000.0</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
    <nineml:initial name="weight">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>2.5</nineml:scalar>
          <nineml:unit>nA</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
  </nineml:component>
  <nineml:component name="nrn">
    <nineml:definition language="NineML">
      <nineml:link>BrunelIAF.xml</nineml:link>
    </nineml:definition>
    <nineml:property name="R">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>1.5</nineml:scalar>
          <nineml:unit>dimensionless</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:property name="Vreset">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>10.0</nineml:scalar>
          <nineml:unit>mV</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:property name="tau">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>20.0</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:property name="tau_rp">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>2.0</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:property name="theta">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>20.0</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:property>
    <nineml:initial name="t_rpend">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>0.0</nineml:scalar>
          <nineml:unit>ms</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
    <nineml:initial name="V">
      <nineml:quantity>
        <nineml:value>
          <nineml:scalar>0.0</nineml:scalar>
          <nineml:unit>mV</nineml:unit>
        </nineml:value>
      </nineml:quantity>
    </nineml:initial>
  </nineml:component>
  <nineml:component name="AllToAll">
    <nineml:definition language="NineML">
      <nineml:link>AllToAllConnection.xml</nineml:link>
    </nineml:definition>
  </nineml:component>
  <nineml:group name="BrunelCaseC">
    <nineml:population name="Ext">
      <nineml:number>1</nineml:number>
      <nineml:prototype>stim</nineml:prototype>
    </nineml:population>
    <nineml:population name="Inh">
      <nineml:number>1</nineml:number>
      <nineml:prototype>nrn</nineml:prototype>
    </nineml:population>
    <nineml:population name="Exc">
      <nineml:number>1</nineml:number>
      <nineml:prototype>nrn</nineml:prototype>
    </nineml:population>
    <nineml:set name="All neurons">
      <nineml:select>
        <nineml:any>
          <nineml:equal>
            <nineml:value>population[@name]</nineml:value>
            <nineml:value>Exc</nineml:value>
          </nineml:equal>
          <nineml:equal>
            <nineml:value>population[@name]</nineml:value>
            <nineml:value>Inh</nineml:value>
          </nineml:equal>
        </nineml:any>
      </nineml:select>
    </nineml:set>
    <nineml:projection name="External">
      <nineml:source>Ext</nineml:source>
      <nineml:target>All neurons</nineml:target>
      <nineml:rule>AllToAll</nineml:rule>
      <nineml:response>syn</nineml:response>
      <nineml:plasticity>ExternalPlasticity</nineml:plasticity>
      <nineml:response_ports>
        <nineml:port_connection port2="Isyn" port1="Isyn"/>
      </nineml:response_ports>
      <nineml:connection_ports>
        <nineml:port_connection port2="q" port1="weight"/>
      </nineml:connection_ports>
    </nineml:projection>
  </nineml:group>
</nineml:nineml>
